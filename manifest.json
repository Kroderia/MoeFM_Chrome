{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "萌否电台MoeFM",
  "description": "MoeFM Chrome Extension.",
  "version": "0.6",
  "icons": {
      "128": "img/logo.png"
  },
  
  
  "permissions": [
      "background",
      "storage",
      "notifications"
  ],

  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "html/index.html"
  },
  
  "background": {
      "scripts": ["js/jquery.js", "js/settings.js", "js/globalfunc.js", "js/background.js"]
  }
}