{
  "manifest_version": 2,

  "name": "萌否电台MoeFM",
  "description": "MoeFM Chrome Extension.",
  "version": "0.5",
  "icons": {
      "128": "img/logo.png"
  },
  
  
  "permissions": [
      "background",
      "storage",
      "notifications"
  ],

  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "html/index.html"
  },
  
  "background": {
      "page": "html/background.html"
  }
}